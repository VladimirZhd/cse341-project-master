<% include ./includes/header-b4.ejs %> 
  </head>
  <body>
    <% include ./includes/nav.ejs %> 
    <div class="container">
      <div class="row">
        <div class="col">
          <ul class="list-group">
            <% for (let pokemon of pokemons) { %>
              <li class="list-group-item"><p class="lead"><%= pokemon.name %></p></li>
            <% } %>  
          </ul>
          <div class="btn-group my-1">
            <% if (prev) { %>
              <form action="/prove/pr09/prev" method="GET">
                <button class="btn btn-primary" type="submit">Previous</button>
              </form>
            <% } %> 
            <% if (next) { %>
              <form action="/prove/pr09/next" method="GET">
                <button class="btn btn-primary ml-1" type="submit">Next</button>
              </form>
            <% } %> 
          </div>
        </div>
      </div>
    </div>
  </body>
</html>
